        <TextBox x:Name="GodinaProizvodnjeTextBox" HorizontalAlignment="Left" Margin="378,226,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" PreviewTextInput="GodinaProizvodnjeTextBox_PreviewTextInput"/>
